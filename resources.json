                            "type": "Git",
                            "git": {
                                "uri": "${GIT_REPOSITORY_URL}",
                                "ref": "${GIT_REFERENCE}"
                            },
                            "contextDir": "${CONTEXT_DIR}"
                        },
                        "strategy": {
                            "type": "Source",
                            "sourceStrategy": {
                                "from": {
                                    "kind": "ImageStreamTag",
